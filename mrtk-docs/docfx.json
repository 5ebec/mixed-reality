{
  "build": {
    "content": [
      {
        "group": "mrtk-unity-all-up",
        "src": "./",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "group": "mrtk-unity-all-up",
        "src": "./",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/mrtk/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "ms.prod": "mixed-reality"
    },
    "fileMetadata": {
      "monikerRange": {
        //"./**/*.md": ">= mrtk-unity-2.5"
      }
    },
    "template": [],
    "dest": "mrtk",
    "markdownEngineName": "markdig",
    "groups": {
      "mrtk-unity-all-up": {
        "dest": "temp-mrtk-unity"
        //"moniker_range": ">= mrtk-unity-2.5"
      }
    }
  }
}
