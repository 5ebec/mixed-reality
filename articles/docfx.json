{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "src": "articles",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.wav"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb"
      },
      {
        "files": [
          "includes/**/*.svg",
          "includes/**/*.png",
          "includes/**/*.jpg",
          "includes/**/*.jpeg",
          "includes/**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
